pipeline{
    agent any
    stages{
        stage('build stage'){
            steps{
                echo "this is the build stage"
            }

        }
        stage('test stage'){
            steps{
                echo "this is the test stage"
            }

        }

        stage('deploy stage'){
            steps{
                echo "this is the deploy stage"
            }

        }
            s
    }



}