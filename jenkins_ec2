pipeline{
    agent any
    stages{
        stage('checkout'){
            steps{
                checkout scmGit(branches: [[name: '*/main']], extensions: [], userRemoteConfigs: [[url: 'https://github.com/Harikrishna241/Terraform.git']])
                sh 'cd Ec2'
                sh 'ls -al'
            }
        
        }
    }
}